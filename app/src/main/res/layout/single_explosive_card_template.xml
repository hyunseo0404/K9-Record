<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.v7.widget.CardView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/view2"
        android:layout_marginTop="15dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="20dp"
            android:id="@+id/textView4"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@color/cardTitle"
                android:id="@+id/relativeLayout2">

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Explosive"
                    android:id="@+id/general_session_title"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true" />

                <ImageButton
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:id="@+id/imageButton"
                    android:src="@drawable/ic_action_add_comment"
                    android:layout_centerVertical="true"
                    android:layout_alignEnd="@+id/general_session_title" />

            </RelativeLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Quantity:"
                android:id="@+id/textView32"
                android:layout_below="@+id/relativeLayout2"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="15dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Location:"
                android:id="@+id/textView33"
                android:layout_below="@+id/textView32"
                android:layout_alignStart="@+id/textView32"
                android:layout_marginTop="30dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Start Time:"
                android:id="@+id/textView34"
                android:layout_below="@+id/textView33"
                android:layout_alignStart="@+id/textView33"
                android:layout_marginTop="43dp" />

            <View
                android:layout_width="1dp"
                android:layout_height="1dp"
                android:id="@+id/textView35"
                android:layout_below="@+id/relativeLayout2"
                android:layout_centerHorizontal="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="End Time:"
                android:id="@+id/textView36"
                android:layout_alignTop="@+id/textView34"
                android:layout_toEndOf="@+id/textView35" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Time to Find:"
                android:id="@+id/textView37"
                android:layout_below="@+id/textView34"
                android:layout_alignStart="@+id/textView34"
                android:layout_marginTop="30dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Success:"
                android:id="@+id/textView38"
                android:layout_alignTop="@+id/textView32"
                android:layout_alignStart="@+id/textView35" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Small Text"
                android:id="@+id/textView39"
                android:layout_below="@+id/textView32"
                android:layout_alignStart="@+id/textView32"
                android:layout_marginTop="3dp"
                android:layout_marginLeft="9dp"
                android:layout_toStartOf="@+id/textView40" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Small Text"
                android:id="@+id/textView40"
                android:layout_below="@+id/textView38"
                android:layout_alignStart="@+id/textView38"
                android:layout_marginTop="3dp"
                android:layout_alignParentEnd="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Small Text"
                android:id="@+id/textView41"
                android:layout_below="@+id/textView33"
                android:layout_alignStart="@+id/textView33"
                android:layout_marginTop="3dp"
                android:layout_marginLeft="9dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Small Text"
                android:id="@+id/textView43"
                android:layout_below="@+id/textView34"
                android:layout_alignStart="@+id/textView34"
                android:layout_marginTop="3dp"
                android:layout_marginLeft="9dp"
                android:layout_alignEnd="@+id/textView39" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Small Text"
                android:id="@+id/textView44"
                android:layout_alignTop="@+id/textView43"
                android:layout_alignStart="@+id/textView36"
                android:layout_marginTop="3dp"
                android:layout_alignEnd="@+id/textView40" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Small Text"
                android:id="@+id/textView45"
                android:layout_below="@+id/textView37"
                android:layout_alignStart="@+id/textView37"
                android:layout_marginTop="3dp"
                android:layout_marginLeft="9dp" />

        </RelativeLayout>

    </android.support.v7.widget.CardView>
</RelativeLayout>
